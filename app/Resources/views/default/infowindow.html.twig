<div class="contents">
    <table class="table table-bordered table-condensed">
        <tr>
            <th>Pedido</th>
            <td>#{{ data.id }}</td>
        </tr>
        <tr>
            <th>Nome</th>
            <td>{{ data.nome }}</td>
        </tr>
        <tr>
            <th>Tempo de entrega</th>
            <td>{{ data.tempoEntrega }}</td>
        </tr>
        <tr>
            <th>Valor do pedido</th>
            <td>R$ {{ data.total }}</td>
        </tr>
    </table>
</div>